{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"]
  ],
  "autoClosingPairs": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"],
    ["\"", "\""]
  ],
  "surroundingPairs": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"],
    ["\"", "\""]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^.*(do|then)\\s*$",
    "decreaseIndentPattern": "^\\s*(end|else)\\b.*$"
  }
}